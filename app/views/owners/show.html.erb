<p id="notice"><%= notice %></p>
<div class="container">
<p>
  <strong>Name:</strong>
  <%= @owner.name %>
</p>

<p>
  <strong>Phone:</strong>
  <%= @owner.phone %>
</p>

<ul>
 <% @owner.dogs.order('name').each do |dogs_owned| %>

 <li><%= link_to dogs_owned.name, dog_path(dogs_owned) %></li>
 <% end %>
</ul>

<%= link_to 'Edit', edit_owner_path(@owner), class: 'btn btn-info' %> |
<%= link_to 'Back', owners_path, class: 'btn btn-info' %>
</div>